{
  "name": "jingqb-design",
  "version": "1.1.13",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.umd.js",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^16.7.0"
  },
  "scripts": {
    "start": "cd example && umi dev",
    "test": "umi-test --verbose=false --no-cache",
    "dev": "father build --watch",
    "build": "father build",
    "pub": "yarn run test && yarn publish",
    "doc:dev": "father doc dev",
    "doc:build": "father doc build",
    "doc:deploy": "father doc deploy",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint",
    "lint-staged:ts": "tslint",
    "prettier": "node ./scripts/prettier.js"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,jsx}": [
      "node ./scripts/lint-prettier.js",
      "git add"
    ],
    "**/*.{js,jsx}": "yarn run lint-staged:js",
    "**/*.{ts,tsx}": "yarn run lint-staged:ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint-staged"
    }
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/echarts": "^4.4.1",
    "@types/prop-types": "^15.7.0",
    "@types/react-dom": "^16.9.4",
    "@types/react-overlays": "^1.1.3",
    "@types/react-slick": "^0.23.4",
    "@types/react-transition-group": "^4.2.3",
    "@types/swiper": "^5.2.0",
    "antd-tools": "^7.0.2",
    "babel-eslint": "^10.0.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-compat": "^2.7.0",
    "father": "^2.29.2",
    "husky": "^1.3.1",
    "lint-staged": "^10.0.7",
    "postcss-url": "^8.0.0",
    "typescript-react-svg-icon-generator": "^1.1.9",
    "umi": "^2.6.0",
    "umi-test": "^1.4.4"
  },
  "dependencies": {
    "@antv/f2": "^3.4.4",
    "classnames": "^2.2.6",
    "echarts": "^4.5.0",
    "omit.js": "^1.0.2",
    "prop-types": "^15.7.2",
    "rc-notification": "^4.0.0-rc.1",
    "react-highcharts": "^16.1.0",
    "react-id-swiper": "^2.4.0",
    "react-overlays": "^2.1.0",
    "react-slick": "^0.25.2",
    "react-transition-group": "^4.3.0",
    "rmc-notification": "^1.0.0",
    "slick-carousel": "^1.8.1",
    "swiper": "^5.2.1"
  }
}
